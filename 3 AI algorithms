    {
      "cell_type": "markdown",
      "source": [
        "**Q1-Q4: Code below implements the K-means algorithm for Dataset 1**"
      ],
      "metadata": {
        "id": "wMoQOtGf6Bc6"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.metrics import pairwise_distances_argmin\n",
        "\n",
        "def find_clusters1(X):\n",
        "\n",
        "    centers = []\n",
        "    centers.append(X.iloc[1])\n",
        "    centers.append(X.iloc[2])\n",
        "    centers.append(X.iloc[7])\n",
        "    centers = pd.DataFrame(centers, columns=['xcord', 'ycord'], index=['A2', 'A3', 'A8'])\n",
        "\n",
        "    plt.figure()\n",
        "    plt.scatter(X['xcord'], X['ycord']);\n",
        "    plt.scatter(centers['xcord'], centers['ycord'], c='red', s=200, alpha=0.5);\n",
        "\n",
        "    plt.title(\"Centroids for data before any epochs ran, dataset1\")\n",
        "    plt.ylabel('Y')\n",
        "    plt.xlabel('X')\n",
        "\n",
        "    epoch = 0\n",
        "    while True:\n",
        "\n",
        "        labels = pairwise_distances_argmin(X, centers)\n",
        "\n",
        "\n",
        "        new_centers = np.array([X[labels == i].mean(0)\n",
        "                                for i in range(3)])\n",
        "\n",
        "        if np.all(centers == new_centers):\n",
        "            print('')\n",
        "            break\n",
        "        centers = new_centers\n",
        "\n",
        "        if epoch == 0:\n",
        "          epoch +=1\n",
        "          temp = centers\n",
        "          temp = pd.DataFrame(temp, columns=['xcord', 'ycord'], index=['A2', 'A3', 'A8'])\n",
        "\n",
        "          plt.figure()\n",
        "          plt.scatter(X['xcord'], X['ycord']);\n",
        "          plt.scatter(temp['xcord'], temp['ycord'], c='red', s=200, alpha=0.5);\n",
        "          plt.title(\"Centroids after 1 epoch, dataset1\")\n",
        "          plt.ylabel('Y')\n",
        "          plt.xlabel('X')\n",
        "\n",
        "    centers = pd.DataFrame(centers, columns=['xcord', 'ycord'], index=['A2', 'A3', 'A8'])\n",
        "    plt.figure()\n",
        "    plt.scatter(X['xcord'], X['ycord']);\n",
        "    plt.scatter(centers['xcord'], centers['ycord'], c='red', s=200, alpha=0.5);\n",
        "    plt.title(\"Centroids after convergence, dataset1\")\n",
        "    plt.ylabel('Y')\n",
        "    plt.xlabel('X')\n",
        "\n",
        "    return centers, labels\n",
        "\n",
        "centers, labels = find_clusters1(data)\n",
        "clust = [\"A1\", \"A2\", \"A3\", \"A4\", \"A5\", \"A6\", \"A7\", \"A8\"]\n",
        "clust_new = pd.DataFrame(labels, columns=[''], index=clust)\n",
        "print(\"Q1. The new clusters are: \")\n",
        "print(clust_new)\n",
        "print(\"\")\n",
        "print(\"Q2. The centers of the new clusters are: \")\n",
        "print(centers)\n",
        "print(\"\")\n",
        "plt.figure()\n",
        ""
